package ex2;

public class ContoCorrente {
	private int numCC;
	private int saldo;
	private int movimenti[];
	private int numMovimenti;
	
	public ContoCorrente(int numCC, int saldo) {
		setNumCC(numCC);
		setSaldo(saldo);
	}

	public void setNumCC(int numCC) {
		this.numCC = numCC;
	}

	public void setSaldo(int saldo) {
		this.saldo = saldo;
	}
}
